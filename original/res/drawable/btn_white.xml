<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:drawable="@drawable/btn_white_disabled" />
    <item android:state_pressed="true" android:drawable="@drawable/btn_white_pressed" />
    <item android:drawable="@drawable/btn_white_selected" android:state_hovered="true" />
    <item android:state_focused="true" android:drawable="@drawable/btn_white_selected" />
    <item android:state_selected="true" android:drawable="@drawable/btn_white_selected" />
    <item android:drawable="@drawable/btn_white_default" />
</selector>